{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/upload",
        "dest": "/app.py"
      },
      {
        "src": "/ask",
        "dest": "/app.py"
      },
      {
        "src": "/get-pdf/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/get-pdf-names",
        "dest": "/app.py"
      }
    ]
  }
  